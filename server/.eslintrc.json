{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": [
    "plugin:node/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": [],
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "strict": "error"
  }
}